.main_daily{border:1px solid #DDDDDD;width:1002px;height:312px;margin-top:24px;margin-left:125px;}
.main_dailyMenu{width:266px;font-size:14px;padding-top: 20px;position: absolute;}
.main_dailyMenu_title{background:url('../images/menu_title.gif');height:37;width:211px;font-size:16px;color:#FFFFFF;
	padding-left: 35px;line-height: 40px;font-weight: bold;margin:0px 0 0 20px;}
.main_dailyMenu ul{list-style-type:none;background:url('../images/subbg.jpg') repeat-x scroll 0 0 #F7FCFF;
	margin:0px 0 0 20px;padding-top:10px; border: 1px solid #A9D5F8;}
.main_dailyMenu ul li{margin-left:7px;margin-top:10px;background:url('../images/blue_arrow.gif');background-repeat:no-repeat;
	padding-left: 10px;line-height: 8px;height:23;width:211px;}
.subNav{background:url("../images/blue_wb.gif");background-repeat:no-repeat;font-size:14px;padding-left:12px;line-height:10px;margin-top:25px;}

.main-management{width:600px;height:200px;position:absolute;margin:0px 0 0 300px;display:block;font-size:14px;}
.main-management p{font-size:16px;margin:50px 0 0 20px;}

.meeting-list{font-size:12px;text-align:center;}
.meeting-list th{background:url("../images/bgx.png");background-repeat:repeat-x;background-position:0 -792px;font-size:12px;
	text-align:center;display:block-inline;border-style:solid;border-width:0;border-color: #969696;height: 24px;
	line-height: 26px;width:100px;margin-top:3px;margin-left:5px;border-radius:3px;}
.meeting_theme{cursor:pointer; text-decoration : underline; color:blue;}
.pages{color: #666666;font-size:13px;margin-top:10px;}

.meeting-detail{width:600px;height:200px;position:absolute;margin:0px 0 0 300px;display:none;font-size:14px;}
.tou{text-align:center;}

.file-management{width:700px;height:200px;position:absolute;margin:0px 0 0 300px;display:block;font-size:14px;}
#key{-moz-border-bottom-colors: none;-moz-border-image: none;-moz-border-left-colors: none;-moz-border-right-colors: none;
    -moz-border-top-colors: none;background: none repeat scroll 0 0 #FFFFFF;border-color: #999999 #C9C9C9 #B5B5B5 #B5B5B5;border-style: solid;
    border-width: 1px;height: 26px;margin-right: -1px;position: relative;}
.sou{background:url("../images/global.png");background-position: -816px -80px;color: #434343;height: 26px;letter-spacing: 3px;
    line-height: 25px;position: relative; text-align: center;text-decoration: none;text-indent: 3px;width: 50px;margin-right:10px;}

.task-management{width:700px;height:200px;position:absolute;margin:0px 0 0 300px;display:block;font-size:14px;}


.recent-bulletin{width:700px;height:200px;position:absolute;margin:0px 0 0 300px;display:block;font-size:14px;}

.duty-list{width:100%;}